{
  "name": "prestashop-classic-dev-tools",
  "version": "1.0.0",
  "description": "Tools to help while developing the Classic theme",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress",
    "watch": "webpack --progress --watch",
    "scss-lint": "stylelint \"**/*.scss\" --formatter verbose",
    "scss-fix": "stylelint \"**/*.scss\" --fix",
    "lint": "eslint -c .eslintrc.js --ext .js,.vue ./js",
    "lint-fix": "eslint -c .eslintrc.js --ext .js,.vue ./js --fix",
    "export": "cd .. && make-dir export && rimraf export/* --glob && cpx *.png export && cpx {assets,config,modules,templates,translations}/**/* export && cd export/ && bestzip %npm_package_name%_v%npm_package_version%.zip *"
  },
  "author": "PrestaShop",
  "license": "AFL-3.0",
  "devDependencies": {
    "@babel/eslint-parser": "^7.25.1",
    "autoprefixer": "^10.4.20",
    "bestzip": "^2.2.1",
    "bootstrap": "4.0.0-alpha.5",
    "bootstrap-touchspin": "^3.1.1",
    "bourbon": "^7.3.0",
    "cpx2": "^7.0.1",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.2.2",
    "eslint": "^8.18.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-webpack": "^0.13.9",
    "eslint-plugin-html": "^8.1.2",
    "eslint-plugin-import": "^2.30.0",
    "expose-loader": "^5.0.0",
    "flexibility": "^2.0.1",
    "jquery": "^3.7.1",
    "jquery-touchswipe": "^1.6",
    "jquery.browser": "^0.1.0",
    "make-dir-cli": "^4.0.0",
    "manrope": "^2.0.0",
    "material-design-icons-iconfont": "^6.7.0",
    "postcss-flexibility": "^3.0.0",
    "postcss-loader": "^8.1.1",
    "rimraf": "^6.0.1",
    "sass": "^1.79.3",
    "sass-loader": "^16.0.2",
    "style-loader": "^4.0.0",
    "stylelint": "^14.5.3",
    "stylelint-config-prestashop": "^1.0.7",
    "tether": "^2.0.0",
    "velocity-animate": "^1.5.2"
  },
  "dependencies": {
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4",
    "webpack-license-plugin": "^4.5.0",
    "webpack-sources": "^3.2.3",
    "terser-webpack-plugin": "^5.3.10",
    "mini-css-extract-plugin": "^2.9.1",
    "csso-webpack-plugin": "^2.0.0-beta.3",
    "events": "^3.3.0",
    "sprintf-js": "^1.1.3"
  }
